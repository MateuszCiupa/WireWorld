<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="153.0" prefWidth="477.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.StartController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="63.0" prefWidth="463.0" text="Wire World Game">
         <font>
            <Font name="Courier New" size="37.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="77.0" text="Start the Game by filling text fields with numbers and clicking Play button." />
      <Button fx:id="playButton" layoutX="14.0" layoutY="116.0" mnemonicParsing="false" onAction="#onPlay" text="Play" />
      <Button fx:id="cancelButton" layoutX="405.0" layoutY="116.0" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
      <TextField fx:id="sizeField" layoutX="102.0" layoutY="116.0" prefHeight="25.0" prefWidth="60.0" />
      <TextField fx:id="delayField" layoutX="223.0" layoutY="116.0" prefHeight="25.0" prefWidth="60.0" />
      <Label layoutX="70.0" layoutY="120.0" text="Size:" />
      <Label layoutX="183.0" layoutY="120.0" text="Delay:" />
   </children>
</AnchorPane>
